# terminal
alias ll='ls -la'
alias ..='cd ..'

# git

# git 
alias g='git'

# git status
alias gs='g status' #

# git add
alias ga='g add'

# git add -A
alias gaa='ga -A'

# git commit -m
alias gc='g commit -m'

# git commit -m "Update application"
alias gcc='g commit -m "Update application"'

# git diff
alias gd='g diff'

# git push
alias gp='g push'

# git push origin
alias gpo='gp origin'

# git push origin master
alias gpom='gpo master'

# git push origin develop 
alias gpod='gpo develop'

# git add -A && git commit -m "Update application" && git push
alias gacp='gaa && gcc && gp'

# git pull
alias gpl='g pull'

# git pull origin master
alias gplom='gpl origin master'

# git pull origin develop
alias gplod='gpl origin develop'

# git branch
alias gb='g branch'

# git checkout
alias gch='g checkout'

# git checkout master
alias gchm='gch master'

# git checkout develop
alias gchd='gch develop'

# git checkout -b
alias gchb='gch -b'

# git reset
alias gr='g reset'

# git rm
alias grm='g rm'

# git rm -r
alias grmr='grm -r'

# git rm -f
alias grmf='grm -f'

# git rm -rf
alias grmrf='g rm -rf'

# git stash
alias gst='g stash'

# git stash apply
alias gsta='gst apply'

# git log
alias gl='g log'

# git log --oneline
alias glol='g log --oneline'

# vue
alias vue='winpty vue.cmd'